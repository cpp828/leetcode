class Solution(object):
    def reverse(self, x):
        """
        :type x: int
        :rtype: int
        """
        ret=0
        r=abs(x)
        while r:
            ret=ret*10+r%10
            r/=10
        if -ret>=-(2**31) and x<0:
            return -(ret)
       
        elif x>=0 and ret<=2**31-1:
            return ret
        else:
            return 0
